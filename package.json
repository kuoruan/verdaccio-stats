{
  "name": "verdaccio-stats",
  "version": "0.4.0",
  "description": "The stats plugin for Verdaccio",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "cross-env DEBUG='verdaccio:*' node_modules/verdaccio/bin/verdaccio -c verdaccio/verdaccio.yml",
    "build": "rimraf dist && rollup -c --environment NODE_ENV:production",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest --run",
    "prepublishOnly": "pnpm run build"
  },
  "repository": "https://github.com/kuoruan/verdaccio-stats.git",
  "keywords": [
    "verdaccio",
    "plugin",
    "stats"
  ],
  "files": [
    "lib"
  ],
  "author": "Xingwang Liao<kuoruan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.25",
    "@types/ms": "^2.1.0",
    "@verdaccio/types": "13.0.0-next-8.10",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-next": "^0.6.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^63.0.0",
    "express": "^4.22.1",
    "globals": "^17.3.0",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "rollup": "^4.57.1",
    "rollup-plugin-node-externals": "^8.1.2",
    "sqlite3": "^5.1.7",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "verdaccio": "^6.2.5",
    "verdaccio-stats": "file:",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "verdaccio": "^5.0.0||^6.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/sequelize": "^4.1.1",
    "@verdaccio/core": "8.0.0-next-8.29",
    "adminjs": "^7.8.17",
    "dayjs": "^1.11.19",
    "debug": "^4.4.3",
    "ms": "^2.1.3",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.8.2",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "^19.0.0"
      }
    },
    "overrides": {
      "@tiptap/core": "^2.7.2",
      "@tiptap/pm": "^2.7.2"
    },
    "onlyBuiltDependencies": [
      "sqlite3"
    ]
  }
}
